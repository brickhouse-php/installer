{
    "name": "brickhouse/installer",
    "description": "Installation wizard for creating new Brickhouse applications",
    "license": "MIT",
    "keywords": ["framework", "brickhouse", "mvc"],
    "homepage": "https://github.com/brickhouse-php/brickhouse",
    "support": {
        "issues": "https://github.com/brickhouse-php/brickhouse/issues",
        "source": "https://github.com/brickhouse-php/brickhouse"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Max T. Kristiansen",
            "email": "me@maxtrier.dk",
            "role": "Author",
            "homepage": "https://maxtrier.dk"
        }
    ],
    "bin": [
        "bin/brickhouse"
    ],
    "require": {
        "php": "^8.4",
        "brickhouse/console": "dev-main",
        "brickhouse/process": "dev-main"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Brickhouse\\Installer\\": "src/"
        }
    }
}
